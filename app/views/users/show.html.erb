<h2>Details of <%= @user.username %></h2>


<h3>Username</h3>
<%= @user.username %>

<h3>Email</h3>
<%= @user.email %>

<h3>About me</h3>
<%= @user.description %>

<h3>Gender</h3>
<%= @user.is_female  %>

